{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'gomoku'",
      "cargo": {
        "args": [
          "build",
          "--bin=gomoku",
          "--package=gomoku"
